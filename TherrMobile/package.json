{
  "name": "Therr",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "export $(cat .env | xargs) && ./node_modules/.bin/react-native run-android",
    "android:clean": "pushd android && ./gradlew clean && popd",
    "android:device": "export $(cat .env | xargs) && adb reverse tcp:7770 tcp:7770 && adb reverse tcp:7743 tcp:7743 && adb reverse tcp:8081 tcp:8081 && ./node_modules/.bin/react-native run-android",
    "android:log": "adb logcat *:S ReactNative:V ReactNativeJS:V",
    "android:deps": "pushd android && ./gradlew :app:dependencies && popd",
    "build:dev": "echo \"Error: no dev build for mobile app\" && exit 0",
    "build:release": "export $(cat .env | xargs) && pushd android && ./gradlew bundleRelease && popd",
    "build:release:apk": "export $(cat .env | xargs) && pushd android && ./gradlew assembleRelease && popd",
    "clean": "npm run clean:env && watchman watch-del-all && rm -rf $TMPDIR/react-native-packager-cache-* &&rm -rf $TMPDIR/metro-bundler-cache-* && rm -rf node_modules/ && npm cache clean --force &&npm install && npm start -- --reset-cache",
    "clean:env": "rm -rf node_modules/.cache/babel-loader/*",
    "env:export": "export $(cat .env | xargs)",
    "getSHA": "pushd android && ./gradlew signingReport && popd",
    "ios": "./node_modules/.bin/react-native run-ios",
    "start": "./node_modules/.bin/react-native start",
    "test": "../node_modules/.bin/jest --no-cache",
    "test:update": "../node_modules/.bin/jest -u",
    "test:watch": "../node_modules/.bin/jest --watch",
    "test:coverage": "../node_modules/.bin/jest --coverage",
    "lint": "../node_modules/.bin/eslint . --ext .js,.jsx,.ts,.tsx ./",
    "lint:fix": "../node_modules/.bin/eslint --ext .js,.jsx,.ts,.tsx --fix ./",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.11.0",
    "@react-native-community/geolocation": "^2.0.2",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/toolbar-android": "0.1.0-rc.2",
    "@react-native-picker/picker": "^1.9.8",
    "@react-navigation/drawer": "^5.9.0",
    "@react-navigation/native": "^5.7.3",
    "@react-navigation/stack": "^5.9.0",
    "appcenter": "4.0.0",
    "appcenter-analytics": "4.0.0",
    "appcenter-crashes": "4.0.0",
    "lottie-ios": "^3.1.8",
    "lottie-react-native": "^3.5.0",
    "react-native": "0.63.2",
    "react-native-android-location-services-dialog-box": "^2.8.2",
    "react-native-animated-loader": "0.0.8",
    "react-native-autolink": "^3.0.0",
    "react-native-country-picker-modal": "^2.0.0",
    "react-native-device-info": "^6.2.0",
    "react-native-dotenv": "^2.4.0",
    "react-native-elements": "^2.2.0",
    "react-native-gesture-handler": "^1.7.0",
    "react-native-gps-state": "^2.0.3",
    "react-native-map-clustering": "^3.4.2",
    "react-native-maps": "^0.27.0",
    "react-native-modal-overlay": "^1.3.1",
    "react-native-permissions": "^2.2.2",
    "react-native-phone-input": "^0.2.4",
    "react-native-reanimated": "^1.11.0",
    "react-native-safe-area-context": "^2.0.3",
    "react-native-screens": "^2.10.1",
    "react-native-vector-icons": "^6.7.0",
    "react-native-webview": "^11.0.2",
    "react-native-youtube-iframe": "^1.4.0"
  },
  "devDependencies": {
    "@react-native-community/eslint-config": "^1.0.0",
    "@types/react-native": "^0.63.0",
    "@types/react-native-dotenv": "^0.2.0",
    "@types/react-native-maps": "^0.24.0",
    "@types/react-native-phone-input": "^0.2.1",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "metro-react-native-babel-preset": "^0.58.0",
    "patch-package": "^6.2.2"
  },
  "rnpm": {
    "assets": [
      "resources/fonts"
    ]
  }
}
