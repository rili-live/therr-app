{
  "name": "Therr",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "./node_modules/.bin/react-native run-android",
    "android:clean": "pushd android && ./gradlew clean && popd",
    "android:device": "adb reverse tcp:7770 tcp:7770 && adb reverse tcp:7743 tcp:7743 && adb reverse tcp:8081 tcp:8081 && ./node_modules/.bin/react-native run-android",
    "android:log": "adb logcat *:S ReactNative:V ReactNativeJS:V",
    "clean": "watchman watch-del-all && rm -rf $TMPDIR/react-native-packager-cache-* &&rm -rf $TMPDIR/metro-bundler-cache-* && rm -rf node_modules/ && npm cache clean --force &&npm install && npm start -- --reset-cache",
    "clean:env": "rm -rf node_modules/.cache/babel-loader/*",
    "getSHA": "pushd android && ./gradlew signingReport && popd",
    "ios": "./node_modules/.bin/react-native run-ios",
    "start": "./node_modules/.bin/react-native start",
    "test": "../node_modules/.bin/jest --no-cache",
    "test:update": "../node_modules/.bin/jest -u",
    "test:watch": "../node_modules/.bin/jest --watch",
    "test:coverage": "../node_modules/.bin/jest --coverage",
    "lint": "../node_modules/.bin/eslint . --ext .js,.jsx,.ts,.tsx ./",
    "lint:fix": "../node_modules/.bin/eslint --ext .js,.jsx,.ts,.tsx --fix ./",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.11.0",
    "@react-native-community/geolocation": "^2.0.2",
