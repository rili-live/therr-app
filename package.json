{
  "name": "rili-app",
  "version": "0.0.1",
  "description": "The main Rili app root",
  "main": "index.js",
  "engines": {
    "npm": ">=6.10.0"
  },
  "scripts": {
    "build:all": "npm run prep && _bin/apply-to-all.sh 'npm run build'",
    "build:all:dev": "npm run prep && _bin/apply-to-all.sh 'npm run build:dev'",
    "clean:all": "rm -rf node_modules && _bin/apply-to-all.sh 'rm -rf node_modules'",
    "copy:env": "_bin/copy-env.sh",
    "lint:all": "_bin/apply-to-all.sh 'npm run lint'",
    "lint:fix:all": "_bin/apply-to-all.sh 'npm run lint:fix'",
    "install:root": "npm run prep && npm install && _bin/apply-to-all.sh 'npm install' rootOnly",
    "install:all": "npm run prep && npm install && _bin/apply-to-all.sh 'npm install'",
    "audit:all": "npm run prep && npm audit fix && _bin/apply-to-all.sh 'npm audit fix'",
    "install:all:ci": "npm run prep && npm ci && _bin/apply-to-all.sh 'npm ci'",
    "k8s:apply": "kubectl apply -f k8s --validate=false",
    "k8s:dev": "skaffold dev",
    "seeds:create": "node --require=./node_modules/dotenv/config ./node_modules/.bin/knex --require @babel/register --knexfile ./rili-servers/users-service/src/store/knexfile.js seed:make $1 dotenv_config_path=./.env",
    "seeds:run": "node --require=./node_modules/dotenv/config ./node_modules/.bin/babel-node ./node_modules/.bin/knex --knexfile ./rili-servers/users-service/src/store/knexfile.js seed:run dotenv_config_path=./.env",
    "update:all": "npm run prep && npm update && _bin/apply-to-all.sh 'npm update'",
    "update:dev:all": "npm run prep && npm update --dev && _bin/apply-to-all.sh 'npm update --dev'",
    "prep": "_bin/prep.sh",
    "test:all": "_bin/apply-to-all.sh 'npm test'",
    "test:coverage:all": "_bin/apply-to-all.sh 'npm run test:coverage'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rili-live/rili-app.git"
  },
  "keywords": [
    "Rili",
    "app"
  ],
  "author": "Rili, Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rili-live/rili-app/issues"
  },
