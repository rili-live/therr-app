{
  "name": "rili-app",
  "version": "0.0.1",
  "description": "The main Rili app root",
  "main": "index.js",
  "scripts": {
    "build:all": "sh _bin/apply-to-all.sh 'npm run build'",
    "clean:all": "rm -rf node_modules && sh _bin/apply-to-all.sh 'rm -rf node_modules'",
    "lint:all": "npm install && sh _bin/apply-to-all.sh 'npm run lint'",
    "install:root": "npm install && sh _bin/apply-to-all.sh 'npm install' rootOnly && sh _bin/apply-to-all.sh 'npm run build' rootOnly",
    "install:all": "npm install && sh _bin/apply-to-all.sh 'npm install'",
    "preinstall": "rm -f package-lock.json*",
    "test:all": "sh _bin/apply-to-all.sh 'npm test'",
    "test:coverage:all": "sh _bin/apply-to-all.sh 'npm run test:coverage'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rili-live/rili-app.git"
  },
  "keywords": [
    "Rili",
    "app"
  ],
  "author": "Rili, Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rili-live/rili-app/issues"
  },
  "homepage": "https://github.com/rili-live/rili-app#readme",
  "devDependencies": {
    "husky": "^1.2.0"
  },
  "dependencies": {
    "@babel/preset-env": "^7.1.6"
  }
}
