  "name": "therr-app",
  "version": "0.0.1",
  "description": "The main Therr app root",
  "main": "index.js",
  "engines": {
    "npm": ">=11.0.0"
  },
  "scripts": {
    "build:all": "npm run prep && _bin/apply-to-all.sh 'npm run build'",
    "build:base": "npm run prep && _bin/apply-to-all.sh 'npm run build' rootOnly",
    "build:changed": "npm run prep && _bin/apply-to-changed.sh 'npm run build'",
    "build:all:dev": "npm run prep && _bin/apply-to-all.sh 'npm run build:dev'",
    "clean:all": "rm -rf node_modules && _bin/apply-to-all.sh 'rm -rf node_modules'",
    "copy:env": "_bin/copy-env.sh",
    "lint:all": "_bin/apply-to-all.sh 'npm run lint'",
    "lint:changed": "_bin/apply-to-changed.sh 'npm run lint'",
    "lint:fix:all": "_bin/apply-to-all.sh 'npm run lint:fix'",
    "lint:fix:changed": "_bin/apply-to-changed.sh 'npm run lint:fix'",
    "install:base": "npm run prep && npm install && _bin/apply-to-all.sh 'npm install' rootOnly",
    "install:all": "npm run prep && npm install --legacy-peer-deps && _bin/apply-to-all.sh 'npm install --legacy-peer-deps'",
    "audit:all": "npm run prep && npm audit fix --legacy-peer-deps && _bin/apply-to-all.sh 'npm audit fix'",
    "install:all:ci": "npm run prep && npm ci --omit=optional --legacy-peer-deps && _bin/apply-to-all.sh 'npm ci --omit=optional --legacy-peer-deps'",
    "install:base:ci": "npm run prep && npm ci --omit=optional --legacy-peer-deps && _bin/apply-to-all.sh 'npm ci --omit=optional --legacy-peer-deps' rootOnly",
    "docker:run:postgres": "docker run --name therr-postgres -e POSTGRES_USER=therr -e POSTGRES_PASSWORD=mysecretpassword -d -p 5431:5432 -v postgresTherrDbData:/var/lib/postgresql/data:delegated,rw postgis/postgis:15-3.4",
    "docker:remove:postgres": "docker stop therr-postgres && docker rm therr-postgres",
    "docker:run:redis": "docker run --name therr-redis -d -p 6380:6379 redis",
    "docker:remove:redis": "docker stop therr-redis && docker rm therr-redis",
    "docker:dev:up": "docker compose -f docker-compose.dev.yml up -d",
    "docker:dev:down": "docker compose -f docker-compose.dev.yml down",
    "docker:dev:build": "docker compose -f docker-compose.dev.yml build --no-cache",
    "docker:dev:logs": "docker compose -f docker-compose.dev.yml logs -f",
    "docker:dev:restart": "docker compose -f docker-compose.dev.yml restart",
    "docker:dev:clean": "docker compose -f docker-compose.dev.yml down -v --remove-orphans",
    "docker:infra:up": "docker compose -f docker-compose.infra.yml up -d",
    "docker:infra:down": "docker compose -f docker-compose.infra.yml down",
    "k8s:apply": "kubectl apply -f k8s/prod --validate=false",
    "k8s:dev": "skaffold dev",
    "seeds:create": "node --require=./node_modules/dotenv/config ./node_modules/.bin/knex --require @babel/register --knexfile ./therr-services/users-service/src/store/knexfile.js seed:make $1 dotenv_config_path=./.env",
    "seeds:run": "node --require=./node_modules/dotenv/config ./node_modules/.bin/babel-node ./node_modules/.bin/knex --knexfile ./therr-services/users-service/src/store/knexfile.js seed:run dotenv_config_path=./.env",
    "update:all": "npm run prep && npm update && _bin/apply-to-all.sh 'npm update'",
    "update:dev:all": "npm run prep && npm update --dev && _bin/apply-to-all.sh 'npm update --dev'",
    "prep": "_bin/prep.sh",
    "test:all": "_bin/apply-to-all.sh 'npm test'",
    "test:unit:all": "_bin/apply-to-all.sh 'npm run test:unit'",
    "test:integration:all": "_bin/apply-to-all.sh 'npm test:unit'",
    "test:changed": "_bin/apply-to-changed.sh 'npm test'",
    "test:coverage:all": "_bin/apply-to-all.sh 'npm run test:coverage'",
    "test:coverage:changed": "_bin/apply-to-changed.sh 'npm run test:coverage'"
  },
