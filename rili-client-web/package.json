{
  "name": "rili-client-web",
  "version": "0.0.1",
  "description": "The web client side code for Rili",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:app && npm run build:server",
    "build:app": "webpack --config webpack.app.config.js --env production",
    "build:server": "webpack --config webpack.server.config.js --env production",
    "build:app:watch": "webpack --config webpack.app.config.js --watch --env development",
    "build:server:watch": "webpack --config webpack.server.config.js --watch --env development",
    "lint:fix": "eslint --ext .jsx,.js --fix ./ && tslint --fix -p ./",
    "lint": "eslint --ext .jsx,.js ./ && tslint -p ./",
    "preinstall": "rm -f package-lock.json*",
    "start": "npm run server:dev",
    "server:dev": "nodemon --watch node build/server-client.js",
    "test": "npm run lint:fix && ../node_modules/.bin/jest --no-cache",
    "test:update": "../node_modules/.bin/jest -u",
    "test:watch": "../node_modules/.bin/jest --watch",
    "test:coverage": "../node_modules/.bin/jest --coverage"
  },
  "keywords": [
    "rili",
    "web"
  ],
  "author": "Rili, Inc.",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "babel-plugin-array-includes": "^2.0.3",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.18.9",
    "raf": "^3.4.1",
    "tslint": "^5.11.0",
    "tslint-loader": "^3.5.4",
    "webpack": "^4.16.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.3"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "express": "^4.16.4",
    "hbs": "^4.0.1",
    "history": "^4.7.2",
    "react-ga": "^2.5.6",
    "react-redux": "^6.0.0",
    "react-router-redux": "^4.0.8",
    "react-transition-group": "^2.5.3",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "socket.io-client": "^2.2.0"
  }
}
