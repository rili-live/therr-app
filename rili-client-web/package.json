{
  "name": "rili-client-web",
  "version": "0.0.1",
  "description": "The web client side code for Rili",
  "main": "index.js",
  "scripts": {
    "build": "webpack --env production",
    "build:watch": "webpack --watch --env production",
    "lint:fix": "eslint --ext .jsx,.js --fix ./ && tslint --fix -p ./",
    "lint": "eslint --ext .jsx,.js ./ && tslint -p ./",
    "preinstall": "rm -f package-lock.json*",
    "start": "npm run server:dev",
    "server": "webpack-dev-server --env development",
    "server:dev": "nodemon --watch webpack.config.js --exec \"npm run server\"",
    "test": "npm run lint:fix && jest --no-cache",
    "test:update": "jest -u",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "rili",
    "web"
  ],
  "author": "Rili, Inc.",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "@types/socket.io-client": "^1.4.32",
    "babel-eslint": "^8.2.1",
    "babel-plugin-array-includes": "^2.0.3",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.18.9",
    "raf": "^3.4.1",
    "tslint": "^5.11.0",
    "webpack": "^4.16.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.3"
  },
  "dependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "socket.io-client": "^2.2.0"
  }
}
