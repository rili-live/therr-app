{
  "name": "rili-library-ui-components",
  "version": "1.0.0",
  "description": "Helper utilities to provide common functions that were abstracted for re-use.",
  "main": "index.js",
  "scripts": {
    "build": "webpack --env production",
    "lint:fix": "eslint --ext .jsx,.js --fix ./",
    "lint": "eslint --ext .jsx,.js ./",
    "preinstall": "rm -f package-lock.json*",
    "test": "npm run lint:fix && jest --no-cache",
    "test:update": "jest -u",
    "test:coverage": "jest --coverage"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "babel-plugin-array-includes": "^2.0.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "jest-each": "^0.3.1",
    "webpack": "^4.16.2",
    "webpack-merge": "^4.1.3"
  },
  "jest": {
    "coverageDirectory": "../coverage/",
    "moduleNameMapper": {
      "^.+\\.(css|scss|svg)$": "identity-obj-proxy"
    },
    "setupFiles": [],
    "setupTestFrameworkScriptFile": "./test-setup.js",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  }
}
