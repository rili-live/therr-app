    "lint": "../../node_modules/.bin/eslint --ext .js,.jsx,.ts,.tsx ./",
    "start": "npm run server:dev",
    "server": "../../node_modules/.bin/webpack-dev-server --env development",
    "server:dev": "../../node_modules/.bin/nodemon --watch webpack.config.js --exec \"npm run server\"",
    "test": "npm run lint:fix && ../../node_modules/.bin/jest --no-cache",
    "test:update": "../../node_modules/.bin/jest -u",
    "test:watch": "../../node_modules/.bin/jest --watch",
    "test:coverage": "../../node_modules/.bin/jest --coverage"
