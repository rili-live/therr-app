{
  "name": "therr-api-gateway",
  "version": "1.0.0",
  "description": "Service for users and authentication",
  "main": "build/index.js",
  "scripts": {
    "build": "../node_modules/.bin/webpack --env production",
    "build:dev": "../node_modules/.bin/webpack --env development",
    "build:watch": "../node_modules/.bin/webpack --watch --env development",
    "dev": "export OTEL_SERVICE_NAME='therr-api-gateway' && npx concurrently \"npm run build:watch\" \"npm start\"",
    "lint:fix": "../node_modules/.bin/eslint --quiet --ext .js,.jsx,.ts,.tsx --fix ./",
    "lint": "../node_modules/.bin/eslint --quiet --ext .js,.jsx,.ts,.tsx ./",
    "start": "../node_modules/.bin/nodemon --require=../node_modules/dotenv/config build/server.js dotenv_config_path=../.env --withSQLLogs",
    "test": "../node_modules/.bin/mocha -r ../node_modules/ts-node/register --config .mocharc.js './tests/**/*.test.ts' --exit",
    "test:unit": "../node_modules/.bin/mocha -r ../node_modules/ts-node/register --config .mocharc.js './tests/unit/**/*.test.ts' --exit",
    "test:integration": "../node_modules/.bin/mocha -r ../node_modules/ts-node/register --config .mocharc.js './tests/integration/**/*.test.ts' --exit",
    "test:coverage": "../node_modules/.bin/nyc --config ./nyc.config.js npm run test"
  },
  "keywords": [
    "therr",
    "server"
  ],
  "author": "Therr, Inc.",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "webpack-cli": "^4.7.2"
  }
}
