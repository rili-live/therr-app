{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "certbot",
      "image": "certbot/certbot",
      "hostname": "certbot",
      "essential": false
    },
    {
      "name": "rili-nginx",
      "image": "rili/nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        },
        {
          "hostPort": 443,
          "containerPort": 443
        }
      ],
      "links": [
        "rili-client-web",
        "rili-server-api",
        "rili-server-socket"
      ]
    },
    {
      "name": "rili-client-web",
      "image": "rili/client-web",
      "hostname": "rili-client-web",
      "essential": false
    },
    {
      "name": "rili-server-api",
      "image": "rili/server-api",
      "hostname": "rili-server-api",
      "essential": false
    },
    {
      "name": "rili-server-socket",
      "image": "rili/server-socket",
      "hostname": "rili-server-socket",
      "essential": false
    },
    {
      "name": "rili-redis-pub",
      "image": "redis:5",
      "hostname": "rili-redis-pub",
      "essential": false
    },
    {
      "name": "rili-redis-sub",
      "image": "redis:5",
      "hostname": "rili-redis-sub",
      "essential": false
    }
  ]
}